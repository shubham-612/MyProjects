package com.cg.inherit;

public class Employee {

	private int id;
	String name;
	
	public Employee() {
		super();
	}

	public Employee(int i, String n)
	{
		this.id = i;
		this.name = n;
	}
	
	public void display()
	{
		System.out.print("id : " + id + " name: " + name);
	}

	public int getId() {
		return id;
	}

	public void setId(int id) {
		this.id = id;
	}

	@Override
	public String toString() {
		return "Employee [id=" + id + ", name=" + name + "]";
	}
}	

