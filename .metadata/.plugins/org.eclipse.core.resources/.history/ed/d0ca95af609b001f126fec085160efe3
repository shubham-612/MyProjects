package com.cg.heartrate;

public class Heartrate {
	
	String str;
	public Heartrate(String str) {
		super();
		this.str = str;
	}
	
	public float restingheartrate(String str)
	{
		String[] resting = str.split("@");
		String res = resting[0];
		Float rest =Float.valueOf(res);  
		return rest;
	}

	
}
